# Defina os detalhes do e-mail
$to = "nnboy2002@gmail.com"
$subject = "Teste de Envio de E-mail"
$body = "Este é um teste de envio de e-mail a partir do PowerShell."

# Construa a URL mailto com os detalhes do e-mail
$mailtoLink = "mailto:$to?subject=$([System.Uri]::EscapeDataString($subject))&body=$([System.Uri]::EscapeDataString($body))"

# Abra o link mailto no navegador padrão
Start-Process $mailtoLink



